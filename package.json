  "rxjs-compat": "^6.0.0",
  "source-map-loader": "^0.2.0",
  "style-loader": "^1.0.1",
  "systemjs-builder": "^0.16.13",
  "timsort": "^0.3.0",
  "to-string-loader": "^1.1.5",
  "ts-helpers": "^1.1.2",
  "tslib": "^2.0.3",
  "tslint": "^5.20.1",
  "tslint-loader": "^3.4.3",
  "typescript": "^3.9.2",
  "uuid": "^7.0.3",
  "vis-data": "^6.6.1",
  "vis-util": "^4.3.4",
  "webpack": "^4.41.3",
  "webpack-cli": "^3.3.10",
  "webpack-dev-server": "^3.11.0",
  "webpack-merge": "^4.1.0",
  "zone.js": "~0.10.3"
 },
 "repository": {
  "type": "git",
  "url": "git+https://github.com/visjs/ngx-vis.git"
 },
 "keywords": [
